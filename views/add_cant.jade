select#inputGroupSelect01.custom-select(name="nums", title="Escoger el numero de acuerdos a agregar a la vez")
    option(value="1") 1
    option(value="2") 2
    option(value="3") 3
    option(value="4") 4
    option(value="5") 5
div.input-group-append
    input#add.btn.btn-outline.btn-success(onclick="on('/agregar:nums=')",type="button", value="Agregar", title="Agregar acuerdos")